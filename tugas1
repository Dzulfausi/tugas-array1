#include <iostream>
using namespace std;
int main(){
  int dzul[5];
  int i, hasil=0;
  for (i=0;i<5;i++){
  cout<<"input nilai ke-"<<(i+1)<<" = ";
  cin>>dzul[i];
  }
  for (i=0;i<5;i++){
  hasil+=dzul[i];
  cout<<"hasil ke- "<<i<<" adalah : "<<hasil<<endl;
  }
  cout<<endl<<"Jumlah keseluruhan : "<< hasil<<endl;
  return 0;
} 
